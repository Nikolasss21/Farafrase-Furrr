{
  "builds": [
    {
      "src": "api/paraphrase.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/script.js",
      "dest": "/public/script.js"
    },
    {
      "src": "/api/paraphrase",
      "dest": "/api/paraphrase.py"
    }
  ],
  "functions": {
    "api/paraphrase.py": {
      "memory": 3008,
      "maxDuration": 300
    }
  }
}
