{
  "builds": [
    {
      "src": "api/paraphrase.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*\\.(css|js))",
      "dest": "/public/$1"
    },
    {
      "src": "/api/paraphrase",
      "dest": "/api/paraphrase.py"
    }
  ]
}
